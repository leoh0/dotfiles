#!/bin/bash

# go path
export GOPATH="$HOME/.go"

# update path
export PATH=/usr/local/bin:${PATH}:/sbin

# add go path
export PATH="/usr/local/go/bin:${GOPATH}/bin:${PATH}"

# add for snap application path
export PATH="/var/lib/snapd/snap/bin:${PATH}"

# add cargo path
export PATH="$HOME/.cargo/bin:${PATH}"
